@model IEnumerable<datalayer.showgroupnewmodel>



<section class="category-section border-radius">
    <header><i class="fa fa-folder-open-o"></i> <h3> دسته بندی ها </h3></header>
    <ul>
        @foreach (var x in Model)
        {
            <li><a href="/group/@x.groupid/@x.grouptitle"> @x.grouptitle<span>@x.pagecount</span></a></li>
        }

    </ul>
</section>
